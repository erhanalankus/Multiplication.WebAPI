
@{
    Layout = null;
}
@model List<CarpYeni.Models.Question>
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Control</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/Areas/Admin/Content/bootstrap.min.css" rel="stylesheet" />


    <!-- MetisMenu CSS -->
    <link href="~/Areas/Admin/Content/metisMenu.min.css" rel="stylesheet" />

    <!-- DataTables CSS -->
    <link href="~/Areas/Admin/Content/dataTables.bootstrap.css" rel="stylesheet" />

    <!-- DataTables Responsive CSS -->
    <link href="~/Areas/Admin/Content/dataTables.responsive.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="~/Areas/Admin/Content/sb-admin-2.css" rel="stylesheet" />

    <!-- Custom Fonts -->
    <link href="~/Areas/Admin/Content/font-awesome.min.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <style>
        .erh {
            float: right;
        }
    </style>
</head>

<body>

    <div id="wrapper">
        <div class="row">
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Unsolved Multiplications
                        @Html.ActionLink("Back to solved questions", "Index", null, new { @class = "btn btn-info erh" })
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="dataTable_wrapper">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-unsolved">
                                <thead>
                                    <tr>
                                        <th>Create Date</th>
                                        <th>Numbers</th>
                                        <th>Result</th>
                                        <th>Difficulty</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.CreateDate</td>
                                            <td>@item.Number1 x @item.Number2</td>
                                            <td>@item.Result</td>
                                            <td>@item.Max</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
    </div>



    <!-- /#wrapper -->
    <!-- jQuery -->
    <script src="~/Areas/Admin/Content/Scripts/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="~/Areas/Admin/Content/Scripts/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Areas/Admin/Content/Scripts/metisMenu.min.js"></script>

    <!-- DataTables JavaScript -->
    <script src="~/Areas/Admin/Content/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Admin/Content/Scripts/dataTables.bootstrap.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="~/Areas/Admin/Content/Scripts/sb-admin-2.js"></script>

    <!-- Page-Level Demo Scripts - Tables - Use for reference -->
    <script>
        $(document).ready(function () {
            $('#dataTables-unsolved').DataTable({
                responsive: true,
                "aaSorting": [],
                "searching": false
            });
        });
    </script>

</body>

</html>
